{
    "compilerOptions": {
        "baseUrl": "./",
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "strict": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "lib": ["esnext", "dom"],
        "esModuleInterop": true,
        "paths": {}
    },
    "exclude": ["webpack*.js", "dist"]
}
